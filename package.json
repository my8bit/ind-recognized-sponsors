{
  "name": "ind-recognized",
  "version": "0.1.0",
  "description": "This application is for the quick search from IND recognized sponsors",
  "scripts": {
    "start": "gulp",
    "build": "gulp build",
    "start:webpack": "webpack-dev-server",
    "test": "mocha --compilers js:babel-core/register",
    "jscs": "gulp jscs",
    "coverage": "./node_modules/babel-cli/bin/babel-node.js node_modules/.bin/babel-istanbul cover _mocha --report text --report html -- test",
    "test:watch": "mocha --compilers js:babel-core/register -w",
    "watch": "npm-watch",
    "coveralls": "./node_modules/babel-cli/bin/babel-node.js node_modules/.bin/babel-istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "watch": {
    "coverage": [
      "client/src/js/**/*.js",
      "test/**/*.js"
    ]
  },
  "author": "Ihor Pavlenko",
  "license": "GPL",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.4.0",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^6.1.2",
    "browser-sync": "^2.2.1",
    "browserify": "^8.1.3",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "coveralls": "^2.11.11",
    "css-loader": "^0.23.1",
    "exorcist": "^0.4.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^5.0.0",
    "gulp": "^3.8.11",
    "gulp-clean-css": "^2.0.12",
    "gulp-htmlhint": "^0.3.1",
    "gulp-jscs": "^3.0.2",
    "gulp-sass": "^2.3.1",
    "gulp-util": "^3.0.3",
    "gulp-webserver": "^0.9.1",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "node-sass": "^3.4.2",
    "npm-watch": "^0.1.5",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "https://github.com/substack/watchify",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "whatwg-fetch": "^1.0.0"
  }
}
