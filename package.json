{
  "name": "ind-recognized",
  "version": "0.1.0",
  "description": "This application is for the quick search from IND recognized sponsors",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack",
    "test": "mocha --compilers js:babel-core/register",
    "eslint": "eslint",
    "jscs": "gulp jscs",
    "coverage": "./node_modules/babel-cli/bin/babel-node.js node_modules/.bin/babel-istanbul cover _mocha --report text --report html -- test",
    "test:watch": "mocha --compilers js:babel-core/register -w",
    "watch": "npm-watch",
    "coveralls": "./node_modules/babel-cli/bin/babel-node.js node_modules/.bin/babel-istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "watch": {
    "coverage": [
      "client/src/js/**/*.js",
      "test/**/*.js"
    ]
  },
  "author": "Ihor Pavlenko",
  "license": "GPL",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.4.0",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "copy-webpack-plugin": "^3.0.1",
    "coveralls": "^2.11.11",
    "css-loader": "^0.24.0",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.1.2",
    "eslint-plugin-standard": "^2.0.0",
    "exorcist": "^0.4.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^5.0.0",
    "html-webpack-plugin": "^2.22.0",
    "istanbul": "^0.4.4",
    "minimatch": "^3.0.2",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "node-sass": "^3.4.2",
    "npm-watch": "^0.1.5",
    "pug": "^2.0.0-beta4",
    "pug-loader": "^2.1.0",
    "pug-walk": "1.0.0",
    "sass-loader": "^3.2.3",
    "sinon": "^1.17.4",
    "minimatch": "^3.0.2",
    "vinyl-source-stream": "^1.0.0",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "bourbon": "^4.2.7",
    "whatwg-fetch": "^1.0.0"
  }
}
